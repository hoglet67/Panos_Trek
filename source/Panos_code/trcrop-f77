      LOGICAL FUNCTION CROP(HITEM,HCOMMAND)
      DOUBLE PRECISION HITEM,HCOMMAND
      COMMON/CHARCOMN/NULL,BLANK
      CHARACTER NULL,BLANK
      CHARACTER ITEM(8),COMMAND(8),IT
      INTEGER ICO
      LOGICAL ALPHA
      CROP = .FALSE.
C
      CALL HUNPAK(HITEM,ITEM(1),8)
      CALL HUNPAK(HCOMMAND,COMMAND(1),8)
      IF (ITEM(1).NE.COMMAND(1)) RETURN
      DO 1 I=2,8
      IT=ITEM(I)
      ICO=ICHAR(COMMAND(I))
      IF (IT.EQ.BLANK.OR.IT.EQ.NULL
     + .OR..NOT.ALPHA(ICO)) GO TO 2
      IF (IT.NE.COMMAND(I)) RETURN
 1    CONTINUE
 2    CROP = .TRUE.
C     RETURN
      END
C
      LOGICAL FUNCTION ALPHA(IKAR)
      INTEGER IKAR
      ALPHA=.FALSE.
      IF(IKAR.GE.65 .AND. IKAR.LE.90) ALPHA=.TRUE.
      RETURN
      END
