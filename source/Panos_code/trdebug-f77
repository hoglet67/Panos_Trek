      SUBROUTINE DEBUGIT
      INCLUDE 'TREKCOM-F77'
C
      COMMON/PLAQ/ISCORE,PERDATE,ISKILL
C
C--------DO SOMETHING HERE
C
      LUN=8
      CALL SCORE
      CALL PLAQUE
      LUN=1
C
      RETURN
      END
C
C
      SUBROUTINE CHEATER
      INCLUDE 'TREKCOM-F77'
      ENERGY=10000
      SHLD=10000
      TORPS=100
      REMRES=10000
      DO 10 I=1,20
      DAMAGE(I)=0
 10   CONTINUE
      CALL SRSCAN
C
      RETURN
      END
C
C
      SUBROUTINE CHEATCL
      INCLUDE 'TREKCOM-F77'
      DO 15 I=1,10
      DO 15 J=1,10
 15   QUAD(I,J)=IHDOT
      QUAD(SECTX,SECTY)=SHIP
      CALL SRSCAN
C
      RETURN
      END
